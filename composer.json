{
    "name": "slince/shopify-api-php",
    "description": "Shopify API Client for PHP",
    "keywords": ["shopify", "restful", "shopify-sdk", "shopify-api", "shopify-client", "shopify-client-php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tao",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=8.2",
        "doctrine/inflector": "^2.0",
        "doctrine/cache": "^2.2",
        "guzzlehttp/guzzle": "^7.0",
        "slince/di": "^3.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0",
        "jms/serializer": "^3.28"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "symfony/property-access": "^4.2|^5.0|^6.0",
        "symfony/filesystem": "^4.2|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\Shopify\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slince\\Shopify\\Tests\\": "./tests"
        }
    }
}
